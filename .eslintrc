{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "jquery": true,
        "es6": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "sourceType": "script",
        "ecmaVersion": 6,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "camelcase": 0,
        "curly": 0,
        "wrap-iife": [
            2,
            "any"
        ],
        "linebreak-style": 2,
        "comma-style": [
            2,
            "last"
        ],
        "new-cap": 2,
        "strict": [
            2,
            "function"
        ],
        "no-undef": 2,
        "no-unused-vars": 0,
        "no-console": 0,
        "arrow-parens": [
            "error",
            "as-needed"
        ]
    },
    "globals": {
        "$w": true,
        "$d": true,
        "$header": true,
        "$main": true,
        "$footer": true,
        "$body": true,
        "$head": true,
        "$": "readonly",
        "jQuery": "readonly",
        "Key": true,
        "Time": true,
        "Laravel": true,
        "grecaptcha": "readonly",
        "moment": "readonly"
    }
}
